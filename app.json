{
  "expo": {
    "name": "Gluco",
    "slug": "gluco",
    "owner": "gluco-solutions-inc",
    "version": "1.0.0",
    "description": "Track meals, glucose, and activity to discover personalized wellness insights.",
    "privacyUrl": "https://desert-butter-582.notion.site/Gluco-Privacy-Policy-2e2ea85c4e478070b60aca1c9c90c8f3",
    "orientation": "portrait",
    "icon": "./assets/images/branding/gluco_app_icon.png",
    "scheme": "gluco",
    "userInterfaceStyle": "light",
    "backgroundColor": "#F2F2F7",
    "newArchEnabled": false,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "52",
      "bundleIdentifier": "ca.gluco.solutions.gluco",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSHealthShareUsageDescription": "Gluco needs access to your health data to help track how sleep, steps, and activity affect your wellness.",
        "NSHealthUpdateUsageDescription": "Gluco needs access to record health data.",
        "NSCameraUsageDescription": "Gluco needs camera access to scan food labels and log meals.",
        "NSPhotoLibraryUsageDescription": "Gluco needs photo library access to select meal photos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true,
        "com.apple.developer.in-app-payments": [
          "merchant.ca.gluco.solutions"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/branding/android-icon-foreground.png",
        "backgroundImage": "./assets/images/branding/android-icon-background.png",
        "monochromeImage": "./assets/images/branding/android-icon-monochrome.png"
      },
      "package": "ca.gluco.solutions.gluco"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/branding/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-health",
        {
          "isBackgroundEnabled": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mascots/gluco_app_mascott/gluco_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#151718",
          "dark": {
            "backgroundColor": "#151718"
          }
        }
      ],
      "./plugins/with-storekit-config"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "5681821f-1ffe-40ab-9339-dd7cb3f409a5"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/5681821f-1ffe-40ab-9339-dd7cb3f409a5"
    }
  }
}